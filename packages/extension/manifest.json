{
  "name": "Paperpod",
  "version": "0.0.1",
  "description": "Convert any article on the web to podcasts!",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_popup": "dist/index.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+S"
      },
      "description": "Save article to Paperpod"
    }
  }
}